{
	"name": "v7-mockup-demo",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"docs:dev": "vitepress dev docs",
		"docs:build": "vitepress build docs",
		"docs:serve": "vitepress serve docs",
		"build": "next build",
		"format": "prettier --write '*.{ts,tsx,js,jsx,scss,json}'",
		"migrate-dev": "dotenv -e .env.local prisma migrate dev",
		"prisma-studio": "dotenv -e .env.local prisma studio",
		"prebuild": "prisma generate",
		"start": "next start",
		"reset": "dotenv -e .env.local prisma migrate reset --preview-feature",
		"type-check": "tsc",
		"type-check-watch": "tsc --watch"
	},
	"gitHooks": {
		"gitHooks": {
			"pre-commit": "lint-staged",
			"pre-push": "npm run type-check"
		}
	},
	"lint-staged": {
		"*.{ts,tsx,js,jsx,scss,json}": [
			"prettier --write"
		],
		"schema.prisma": [
			"npx prisma format"
		]
	},
	"prisma": {
		"seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
	},
	"dependencies": {
		"@chakra-ui/icons": "^2.0.19",
		"@chakra-ui/next-js": "^2.1.4",
		"@chakra-ui/react": "^2.7.0",
		"@emotion/react": "^11.11.1",
		"@emotion/styled": "^11.11.0",
		"@prisma/client": "^4.15.0",
		"@supabase/supabase-js": "^2.25.0",
		"diacritics": "^1.3.0",
		"framer-motion": "^10.12.16",
		"jsonwebtoken": "^9.0.0",
		"next": "13.4.5",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@types/diacritics": "^1.3.1",
		"@types/jsonwebtoken": "^9.0.2",
		"@types/node": "20.3.1",
		"@types/react": "18.2.12",
		"@types/react-dom": "18.2.5",
		"@types/uuid": "^9.0.2",
		"dotenv-cli": "^7.2.1",
		"lint-staged": "^13.2.2",
		"prettier": "^2.8.8",
		"prisma": "^4.15.0",
		"supabase": "^1.68.6",
		"ts-node": "^10.9.1",
		"typescript": "5.1.3",
		"vitepress": "^1.0.0-beta.3"
	}
}
