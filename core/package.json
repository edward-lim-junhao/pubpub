{
	"name": "core",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"migrate-dev": "dotenv -e .env.local prisma migrate dev",
		"migrate-deploy": "dotenv -e .env.local prisma migrate deploy",
		"prisma-studio": "dotenv -e .env.local prisma studio",
		"prebuild": "prisma generate",
		"start": "next start",
		"reset": "dotenv -e .env.local prisma migrate reset --preview-feature",
		"type-check": "tsc",
		"type-check-watch": "tsc --watch"
	},
	"prisma": {
		"seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
	},
	"dependencies": {
		"@prisma/client": "^4.15.0",
		"@supabase/supabase-js": "^2.25.0",
		"clsx": "^2.0.0",
		"diacritics": "^1.3.0",
		"jsonwebtoken": "^9.0.0",
		"next": "13.4.5",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"tailwind-merge": "^1.14.0",
		"ui": "workspace:*",
		"utils": "workspace:*",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@types/diacritics": "^1.3.1",
		"@types/jsonwebtoken": "^9.0.2",
		"@types/node": "20.3.1",
		"@types/react": "18.2.12",
		"@types/react-dom": "18.2.5",
		"@types/uuid": "^9.0.2",
		"autoprefixer": "^10.4.14",
		"dotenv-cli": "^7.2.1",
		"encoding": "^0.1.13",
		"postcss": "^8.4.27",
		"prisma": "^4.15.0",
		"supabase": "^1.68.6",
		"tailwindcss": "^3.3.3",
		"ts-node": "^10.9.1",
		"typescript": "5.1.3",
		"tsconfig": "workspace:*"
	}
}
