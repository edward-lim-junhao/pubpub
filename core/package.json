{
	"name": "core",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "next dev -p 3000",
		"build": "SKIP_VALIDATION=true next build",
		"invite-users": "dotenv -e .env.local -e .env.development  tsx scripts/invite.ts",
		"lint": "next lint",
		"migrate-dev": "dotenv -e .env.local -e .env.development prisma migrate dev",
		"migrate-deploy": "dotenv -e .env.local -e .env.development prisma migrate deploy",
		"migrate-docker": "dotenv -e .env.docker -- prisma migrate deploy",
		"prisma-studio": "dotenv -e .env.local -e .env.development prisma studio",
		"prebuild": "prisma generate",
		"start": "next start",
		"reset": "dotenv -e .env.local -e .env.development prisma migrate reset --preview-feature",
		"type-check": "tsc",
		"type-check-watch": "tsc --watch"
	},
	"files": [
		".next",
		"public"
	],
	"prisma": {
		"seed": "tsx prisma/seed.ts"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.445.0",
		"@aws-sdk/s3-request-presigner": "^3.445.0",
		"@faker-js/faker": "^8.0.2",
		"@honeycombio/opentelemetry-node": "^0.6.1",
		"@hookform/resolvers": "^3.3.1",
		"@opentelemetry/auto-instrumentations-node": "^0.41.1",
		"@prisma/client": "^5.2.0",
		"@sentry/nextjs": "^7.102.0",
		"@stoplight/elements": "^7.12.2",
		"@supabase/supabase-js": "^2.33.2",
		"@t3-oss/env-nextjs": "^0.9.2",
		"@ts-rest/core": "^3.28.0",
		"@ts-rest/next": "^3.28.0",
		"@ts-rest/open-api": "^3.28.0",
		"ajv": "^8.12.0",
		"clsx": "^2.0.0",
		"contracts": "workspace:*",
		"diacritics": "^1.3.0",
		"eta": "^3.1.1",
		"graphile-worker": "^0.15.0",
		"jsonwebtoken": "^9.0.0",
		"next": "13.5.2",
		"next-connect": "^1.0.0",
		"next-runtime-env": "2",
		"nodemailer": "^6.9.5",
		"openapi3-ts": "^4.1.2",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-hook-form": "^7.46.1",
		"remove": "^0.1.5",
		"server-only": "^0.0.1",
		"tailwind-merge": "^1.14.0",
		"ui": "workspace:*",
		"utils": "workspace:*",
		"uuid": "^9.0.0",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@preconstruct/next": "^4.0.0",
		"@tailwindcss/forms": "^0.5.6",
		"@types/diacritics": "^1.3.1",
		"@types/jsonwebtoken": "^9.0.2",
		"@types/node": "20.3.1",
		"@types/nodemailer": "^6.4.9",
		"@types/react": "18.2.12",
		"@types/react-dom": "18.2.5",
		"@types/uuid": "^9.0.2",
		"autoprefixer": "^10.4.14",
		"csv-parse": "^5.5.2",
		"dotenv-cli": "^7.2.1",
		"postcss": "^8.4.27",
		"prisma": "^5.2.0",
		"supabase": "^1.68.6",
		"tailwindcss": "^3.3.3",
		"ts-node": "^10.9.1",
		"tsconfig": "workspace:*",
		"tsx": "^3.13.0",
		"typescript": "5.1.3",
		"yargs": "^17.7.2"
	}
}
